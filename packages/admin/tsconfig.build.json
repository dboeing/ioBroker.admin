{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "noEmit": false,
    "declaration": false,
    "skipLibCheck": true,
    "module": "commonjs"
  },
  "include": [
    "src-backend/**/*.ts",
    "src-backend/**/*.js"
  ],
  "exclude": [
    "src-backend/**/*.d.ts"
  ]
}